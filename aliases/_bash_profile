# Append Aliases by https://github.com/myun2/my_env"
myun2_myenv_dir=~/my_env
myun2_myenv_aliases_dir=$myun2_myenv_dir/aliases

# Aliases
source $myun2_myenv_aliases_dir/prompt
source $myun2_myenv_aliases_dir/others
source $myun2_myenv_aliases_dir/git
source $myun2_myenv_aliases_dir/tmux
source $myun2_myenv_aliases_dir/rails
source $myun2_myenv_aliases_dir/vim

# Prompt
PS_BODY="$_PROMPT_USER@$_PROMPT_HOST $_PROMPT_CURDIR"
PS_COLOR=$CL_DCYAN
PS1="$PS_COLOR[$PS_BODY]$_PROMPT_MARK$PS_RESET "

export LANG=ja_JP.UTF-8
